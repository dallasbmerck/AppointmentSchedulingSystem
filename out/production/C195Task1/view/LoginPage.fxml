<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginPageController">
   <children>
      <Label fx:id="ApplicationTitleLabel" layoutX="14.0" layoutY="14.0" text="Customer Scheduling Application">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="UserNameLabel" layoutX="197.0" layoutY="123.0" text="Username:" />
      <Label fx:id="PasswordLabel" layoutX="199.0" layoutY="166.0" text="Password:" />
      <TextField fx:id="UsernameText" layoutX="270.0" layoutY="119.0" />
      <TextField fx:id="PasswordText" layoutX="270.0" layoutY="162.0" />
      <Button fx:id="ResetButton" layoutX="278.0" layoutY="210.0" mnemonicParsing="false" onAction="#PressResetButton" text="Reset" />
      <Button fx:id="SignInButton" layoutX="358.0" layoutY="210.0" mnemonicParsing="false" onAction="#PressSignInButton" text="Sign In" />
      <Button fx:id="ExitButton" layoutX="534.0" layoutY="361.0" mnemonicParsing="false" onAction="#PressExitButton" text="Exit" />
      <Label fx:id="ZoneIDLabel" layoutX="547.0" layoutY="19.0" text="ZoneID" />
   </children>
</AnchorPane>
