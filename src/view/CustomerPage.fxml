<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="607.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CustomerController">
   <children>
      <Label layoutX="7.0" layoutY="7.0" text="Customers">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TableView fx:id="customerTableView" layoutX="7.0" layoutY="42.0" prefHeight="290.0" prefWidth="594.0">
        <columns>
          <TableColumn fx:id="customerIDCol" text="Customer_ID" />
          <TableColumn fx:id="customerNameCol" prefWidth="97.0" text="Customer_Name" />
            <TableColumn fx:id="countryCol" prefWidth="54.0" text="Country" />
            <TableColumn fx:id="divisionCol" prefWidth="63.0" text="Division" />
            <TableColumn fx:id="addressCol" prefWidth="131.0" text="Address" />
            <TableColumn fx:id="postCodeCol" prefWidth="71.0" text="Postal Code" />
            <TableColumn fx:id="phoneCol" prefWidth="96.0" text="Phone Number" />
        </columns>
      </TableView>
      <Button fx:id="addButton" layoutX="300.0" layoutY="353.0" mnemonicParsing="false" onAction="#clickAddButton" prefHeight="25.0" prefWidth="52.0" text="Add" />
      <Button fx:id="editButton" layoutX="377.0" layoutY="353.0" mnemonicParsing="false" onAction="#clickEditButton" prefHeight="25.0" prefWidth="52.0" text="Edit" />
      <Button fx:id="deleteButton" layoutX="456.0" layoutY="353.0" mnemonicParsing="false" onAction="#clickDeleteButton" prefHeight="25.0" prefWidth="52.0" text="Delete" />
      <Button fx:id="backButton" layoutX="534.0" layoutY="353.0" mnemonicParsing="false" onAction="#clickBackButton" prefHeight="25.0" prefWidth="52.0" text="Back" />
   </children>
</AnchorPane>
